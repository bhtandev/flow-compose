{
  "name": "flow-compose",
  "version": "1.0.11",
  "description": "All purpose composer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "clean": "rm -f {src}/*.js* {src}/*.d.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhtandev/flow-compose.git"
  },
  "keywords": [
    "middleware",
    "compose",
    "flow",
    "async",
    "pipe",
    "typescript"
  ],
  "author": "Boon Hui Tan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhtandev/flow-compose/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/bhtandev/flow-compose#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {}
}
